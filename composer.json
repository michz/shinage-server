{
  "name": "mztx/shinage-server",
  "license": "MIT",
  "type": "project",
  "description": "Digital signage server component based on Symfony.",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "shinage\\server\\behat\\": "features/bootstrap/"
    },
    "classmap": [
      "src/Kernel.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "spec\\App\\": "spec/",
      "DoctrineMigrations\\": "src/Migrations/"
    }
  },
  "require": {
    "php": ">=8.2",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-json": "*",

    "doctrine/orm": "^2.7",
    "doctrine/doctrine-bundle": "^2.1",
    "studio-42/elfinder": "^2.1",
    "guzzlehttp/guzzle": "^7.4",
    "knplabs/knp-menu-bundle": "^3.0",
    "jms/serializer": "^3.23",
    "jms/serializer-bundle": "^5.4",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "mschop/pathogen": "^0.7.1",
    "symfony/dependency-injection": "6.4.*",
    "symfony/config": "6.4.*",
    "symfony/flex": "^2.0",
    "symfony/twig-bundle": "6.4.*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/form": "6.4.*",
    "symfony/security-bundle": "6.4.*",
    "symfony/translation": "6.4.*",
    "symfony/templating": "6.4.*",
    "symfony/validator": "6.4.*",
    "symfony/dotenv": "6.4.*",
    "symfony/asset": "6.4.*",
    "symfony/mime": "6.4.*",
    "symfony/expression-language": "6.4.*",
    "scheb/2fa-bundle": "^v7.2",
    "scheb/2fa-email": "^v7.2",
    "scheb/2fa-google-authenticator": "^v7.2",
    "scheb/2fa-backup-code": "^v7.2",
    "endroid/qr-code-bundle": "^4.0",
    "symfony/cache": "6.4.*",
    "psr/simple-cache": "^3.0",
    "composer/package-versions-deprecated": "1.11.99.5",
    "symfony/proxy-manager-bridge": "6.4.*",
    "symfony/mailer": "6.4.*"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "6.4.*",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "phpspec/phpspec": "^7.2",
    "behat/behat": "^3.7",
    "friends-of-behat/mink-extension": "^2.3",
    "friends-of-behat/symfony-extension": "^2.0",
    "symplify/easy-coding-standard": "^11.0",
    "cweagans/composer-patches": "~1.0",
    "symfony/stopwatch": "6.4.*",
    "symfony/web-profiler-bundle": "6.4.*",
    "behat/mink-browserkit-driver": "^2.2",
    "rector/rector": "^1.2",
    "phpstan/phpstan": "^1.12",
    "shipmonk/composer-dependency-analyser": "^1.7"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "php install-git-hooks.php"
    ],
    "post-update-cmd": [
      "php install-git-hooks.php"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "config": {
    "platform": {
      "php": "8.2.15"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "endroid/installer": true,
      "cweagans/composer-patches": true,
      "symfony/flex": true
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "6.4.*"
    },
    "patches": {
    },
    "composer-exit-on-patch-failure": true
  }
}
