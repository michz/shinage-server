{
  "name": "mztx/shinage-server",
  "license": "MIT",
  "type": "project",
  "description": "Digital signage server component based on Symfony.",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "shinage\\server\\behat\\": "features/bootstrap/"
    },
    "classmap": [
      "src/Kernel.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "spec\\App\\": "spec/",
      "DoctrineMigrations\\": "src/Migrations/"
    }
  },
  "require": {
    "php": ">=8.3",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "composer/package-versions-deprecated": "1.11.99.5",
    "doctrine/cache": "^2.2",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.2",
    "doctrine/doctrine-bundle": "^2.1",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/migrations": "^3.9",
    "doctrine/orm": "^3.3",
    "endroid/qr-code": "^6.0",
    "endroid/qr-code-bundle": "^6.0",
    "guzzlehttp/guzzle": "^7.4",
    "jms/serializer": "^3.23",
    "jms/serializer-bundle": "^5.4",
    "knplabs/knp-menu": "^3.7",
    "knplabs/knp-menu-bundle": "^3.5",
    "mschop/pathogen": "^0.7.1",
    "psr/log": "^3.0",
    "scheb/2fa-backup-code": "^v7.2",
    "scheb/2fa-bundle": "^v7.2",
    "scheb/2fa-email": "^v7.2",
    "scheb/2fa-google-authenticator": "^v7.2",
    "studio-42/elfinder": "^2.1",
    "symfony/asset": "*",
    "symfony/cache": "*",
    "symfony/config": "*",
    "symfony/console": "*",
    "symfony/dependency-injection": "*",
    "symfony/doctrine-bridge": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "^2.0",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-foundation": "*",
    "symfony/http-kernel": "*",
    "symfony/mailer": "*",
    "symfony/mime": "*",
    "symfony/monolog-bundle": "^3.5",
    "symfony/options-resolver": "*",
    "symfony/password-hasher": "*",
    "symfony/proxy-manager-bridge": "*",
    "symfony/routing": "*",
    "symfony/security-bundle": "*",
    "symfony/security-core": "*",
    "symfony/security-http": "*",
    "symfony/service-contracts": "^3.5",
    "symfony/translation": "*",
    "symfony/translation-contracts": "^3.5",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "twig/twig": "^3.20"
  },
  "require-dev": {
    "behat/behat": "^3.7",
    "behat/gherkin": "^4.12",
    "behat/mink-browserkit-driver": "^2.2",
    "cweagans/composer-patches": "~1.0",
    "doctrine/data-fixtures": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "friends-of-behat/mink-extension": "^2.3",
    "friends-of-behat/symfony-extension": "^2.0",
    "phpspec/phpspec": "8.0.x-dev",
    "phpspec/prophecy": "^1.20",
    "phpstan/phpstan": "^2.1",
    "psr/http-message": "^2.0",
    "rector/rector": "^2.0",
    "shipmonk/composer-dependency-analyser": "^1.7",
    "symfony/phpunit-bridge": "*",
    "symfony/stopwatch": "*",
    "symfony/web-profiler-bundle": "*",
    "symplify/easy-coding-standard": "^12.0",
    "webmozart/assert": "^1.11"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "php install-git-hooks.php"
    ],
    "post-update-cmd": [
      "php install-git-hooks.php"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "config": {
    "platform": {
      "php": "8.4.10"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "endroid/installer": true,
      "cweagans/composer-patches": true,
      "symfony/flex": true
    },
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.2.*"
    },
    "patches": {
    },
    "composer-exit-on-patch-failure": true
  }
}
